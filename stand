-- Anti Afk Ril --
local vu = game:GetService("VirtualUser")
game:GetService("Players").LocalPlayer.Idled:connect(function()
vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
wait(1)
vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)
game:GetService("Players").LocalPlayer.CameraMaxZoomDistance = 9e9
-- AntiCheat Bypass --
assert(getrawmetatable)
gmt = getrawmetatable(game)
setreadonly(gmt, false)
old = gmt.__namecall
gmt.__namecall =
    newcclosure(
        function(self, ...)
        local args = {...}
        if tostring(args[1]) == "BreathingHAMON" then
            return
        elseif tostring(args[1]) == "TeleportDetect" then
            return
        elseif tostring(args[1]) == "CHECKER_1" then
            return
        elseif tostring(args[1]) == "CHECKER" then
            return
        elseif tostring(args[1]) == "GUI_CHECK" then
            return
        elseif tostring(args[1]) == "OneMoreTime" then
            return
        elseif tostring(args[1]) == "checkingSPEED" then
            return
        elseif tostring(args[1]) == "BANREMOTE" then
            return
        elseif tostring(args[1]) == "PERMAIDBAN" then
            return
        elseif tostring(args[1]) == "KICKREMOTE" then
            return
        elseif tostring(args[1]) == "BR_KICKPC" then
            return
        end
        return old(self, ...)
        end)



        game.Players.LocalPlayer.Character:WaitForChild("GRABBING_CONSTRAINT")
        if game.Players.LocalPlayer.Character:FindFirstChild("GRABBING_CONSTRAINT") then
        game:GetService("Workspace").FallenPartsDestroyHeight = -500
        game.Players.LocalPlayer.Character.Humanoid.MaxHealth = 9e9
        wait(0.2)
        game.Players.LocalPlayer.Character.Humanoid.RigType = "R6"
        wait(0.3)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0,-450,0)
        wait(2)
        game:GetService("Workspace").FallenPartsDestroyHeight = math.huge-math.huge
        end

loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()

