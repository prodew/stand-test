tool.AddButton("Pro Bat FLing!",function()
    function Ringtone(ID)
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Save Tool
        local Tool = nil
        if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
        Tool = game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
        game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Parent = game:GetService("Players").LocalPlayer.Backpack
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Phone
        game:GetService("Players").LocalPlayer.Backpack["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Character
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Fire Event
        local args = {
            [1] = "RingTone",
            [2] = ID
            
        }
        game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Unequip Phone
        game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Backpack
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Saved Tool
        if Tool ~= true then
        if Tool then
        Tool.Parent = game:GetService("Players").LocalPlayer.Character
        end
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Play Sound 
        game.Players:chat("/e cm")
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Stop Audio
        function Stop()
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Save Tool
        local Tool = nil
        if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
        Tool = game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
        game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Parent = game:GetService("Players").LocalPlayer.Backpack
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Phone
        game:GetService("Players").LocalPlayer.Backpack["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Character
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Unequip Phone
        game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Backpack
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Saved Tool
        if Tool ~= true then
        if Tool then
        Tool.Parent = game:GetService("Players").LocalPlayer.Character
        end
        end
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Change Id
        function Change(ID)
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Save Tool
        local Tool = nil
        if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
        Tool = game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
        game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Parent = game:GetService("Players").LocalPlayer.Backpack
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Phone
        game:GetService("Players").LocalPlayer.Backpack["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Character
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Fire Event
        local args = {
            [1] = "RingTone",
            [2] = ID
            
        }
        game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Unequip Phone
        game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Backpack
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Saved Tool
        if Tool ~= true then
        if Tool then
        Tool.Parent = game:GetService("Players").LocalPlayer.Character
        end
        end
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Call
        function Call()
        game.Players:chat("/e cm")
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- End
        -- Ringtone(ID)
        -- Stop()
        -- Change(ID)
        -- Call()
    
        getgenv().itohrohrohrohrohrohrohrohrohr = 999
        getgenv().rthrthrthrjrt = 380
     
    
    
    
    
        getgenv().BatFling = false
        game:GetService("RunService").heartbeat:Connect(function()
        if getgenv().BatFling == true then 
            local Vel = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity
            game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector.X*getgenv().itohrohrohrohrohrohrohrohrohr,getgenv().itohrohrohrohrohrohrohrohrohr,game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector.Z*getgenv().itohrohrohrohrohrohrohrohrohr)
            game:GetService("RunService").RenderStepped:Wait()
            game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vel
        end 
        end)
    game.Players.LocalPlayer.Backpack["[Bat]"].Activated:connect(function()
    local Animation = Instance.new("Animation")
    Animation.AnimationId = "rbxassetid://3172948973"
    local LoadAnimation = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):LoadAnimation(Animation)
    LoadAnimation.Looped = false
    LoadAnimation.Priority = Enum.AnimationPriority.Action
    LoadAnimation:Play()
    LoadAnimation:AdjustSpeed(1.43)
    wait(0.25)
    Change(5696182212)
    wait(0.1)
    Call()
    getgenv().BatFling = true
    wait(1.5)
    getgenv().BatFling = false
    end)
end)
