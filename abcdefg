    elseif key == 'b' then
        Ringtone(9068077052)
        wait(0.1)
        Call()
    elseif key == 'n' then
        Ringtone(8280196339)--2738830850---265201042
        wait(0.1)
        Call()
    elseif key == 'y' then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 999999, 20)
        wait(0.13)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, -999994, 0)
        Ringtone(265201042)--2738830850---265201042
        wait(0.1)
        Call()
