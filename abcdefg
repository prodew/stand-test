    function Anti()
        while wait() do
        if game.Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid').Health <= 50 then
                local savepos = game.Players.LocalPlayer.Character.UpperTorso.Position
                         local Brokie = game.Workspace.Ignored.Shop["[Taco] - $2"]
                         game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(584, 40, -474)
                         wait(0.25)
                         fireclickdetector(Brokie.ClickDetector)
                         wait(0.25)
                     wait(.25)
                 local surg = game.Players.LocalPlayer.Backpack:FindFirstChild("[Taco]")
                  surg.Parent = game.Players.LocalPlayer.Character
                  local New = game.Players.LocalPlayer.Character:FindFirstChild("[Taco]")
                  for i = 1,55  do      
                  New:Activate()
                  wait(0.1)
                  New:Activate()
                  end
                  game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(savepos)
        end
        end
        end
        
        game.Players.LocalPlayer.Character:WaitForChild("FULLY_LOADED_CHAR")
            Anti()
        
        game.Players.LocalPlayer.CharacterAdded:connect(function()
            game.Players.LocalPlayer.Character:WaitForChild("FULLY_LOADED_CHAR")
            wait(0.5)
            Anti()
        end)
end)

tool.AddButton("Knife sound!",function()
    function Ringtone(ID)
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Save Tool
        local Tool = nil
        if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
        Tool = game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
        game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Parent = game:GetService("Players").LocalPlayer.Backpack
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Phone
        game:GetService("Players").LocalPlayer.Backpack["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Character
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Fire Event
        local args = {
            [1] = "RingTone",
            [2] = ID
            
        }
        game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Unequip Phone
        game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Backpack
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Saved Tool
        if Tool ~= true then
        if Tool then
        Tool.Parent = game:GetService("Players").LocalPlayer.Character
        end
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Play Sound 
        game.Players:chat("/e cm")
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Stop Audio
        function Stop()
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Save Tool
        local Tool = nil
        if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
        Tool = game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
        game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Parent = game:GetService("Players").LocalPlayer.Backpack
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Phone
        game:GetService("Players").LocalPlayer.Backpack["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Character
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Unequip Phone
        game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Backpack
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Saved Tool
        if Tool ~= true then
        if Tool then
        Tool.Parent = game:GetService("Players").LocalPlayer.Character
        end
        end
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Change Id
        function Change(ID)
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Save Tool
        local Tool = nil
        if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
        Tool = game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
        game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Parent = game:GetService("Players").LocalPlayer.Backpack
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Phone
        game:GetService("Players").LocalPlayer.Backpack["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Character
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Fire Event
        local args = {
            [1] = "RingTone",
            [2] = ID
            
        }
        game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Unequip Phone
        game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Backpack
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Saved Tool
        if Tool ~= true then
        if Tool then
        Tool.Parent = game:GetService("Players").LocalPlayer.Character
        end
        end
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Call
        function Call()
        game.Players:chat("/e cm")
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- End
        -- Ringtone(ID)
        -- Stop()
        -- Change(ID)
        -- Call()
    
    
    
    
    
    
        getgenv().BatFling = false
        game:GetService("RunService").heartbeat:Connect(function()
        if getgenv().BatFling == true then 
        local Vel = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity
        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0) * (0)
        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vel
        end 
        end)
    game.Players.LocalPlayer.Backpack["[Knife]"].Activated:connect(function()
    local Animation = Instance.new("Animation")
    Animation.AnimationId = "rbxassetid://3033700364"
    local LoadAnimation = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):LoadAnimation(Animation)
    LoadAnimation.Looped = false
    LoadAnimation.Priority = Enum.AnimationPriority.Action
    LoadAnimation:Play()
    LoadAnimation:AdjustSpeed(1.43)
    wait(0.25)
    Change(7674672480)
    wait(0.1)
    Call()
    getgenv().BatFling = true
    wait(1.5)
    getgenv().BatFling = false
    end)
end)
tool.AddButton("FPS FPS!",function()
    for _,v in pairs(workspace:GetDescendants()) do
        if v.ClassName == "Part"
            or v.ClassName == "SpawnLocation"
            or v.ClassName == "WedgePart"
            or v.ClassName == "Terrain"
            or v.ClassName == "MeshPart" then
            v.Material = "Plastic"
        end
    end
end)
