            TweenService:Create(blur, TweenInfo.new(0.8), {Size = 0}):Play()
            wait(0.3)
            TweenService:Create(game:GetService("Lighting").ColorCorrection, TweenInfo.new(0.8), {Saturation = 0}):Play()
    elseif key == 't' then
        Ringtone(374106817)
        wait(0.1)
        Call()
    elseif key == 'e' then
        wait(0.2)
        Ringtone(7361042352)--5773418559--943422981--131313234
        wait(0.1)
        Call()
    elseif key == 'v' then
        Ringtone(291315892)
        wait(0.1)
