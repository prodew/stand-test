    game.ReplicatedStorage.MainEvent:FireServer("JoinCrew",32570691)
    workspace.MAP.Map:GetChildren()[119]:GetChildren()[22]:Destroy()
    workspace.MAP.Map:GetChildren()[119].Seat:Destroy()
    game:GetService("Players").LocalPlayer.Backpack.Wallet:Destroy()
    game:GetService("Players").LocalPlayer.Backpack["[Boombox]"]:Destroy()
end)
