tool.AddButton("COD ZOMBIE",function()
    function MoveTo(X,Y,Z)
        OWNER.Character.HumanoidRootPart.CFrame = OWNER.Character.HumanoidRootPart.CFrame*CFrame.new(X,Y,Z)
    end
    TweenService = game:GetService("TweenService")
    OWNER = game:GetService("Players").LocalPlayer
    
    function Ringtone(ID)
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Save Tool
        local Tool = nil
        if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
        Tool = game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
        game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Parent = game:GetService("Players").LocalPlayer.Backpack
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Phone
        game:GetService("Players").LocalPlayer.Backpack["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Character
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Fire Event
        local args = {
            [1] = "RingTone",
            [2] = ID
            
        }
        game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Unequip Phone
        game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Backpack
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Saved Tool
        if Tool ~= true then
        if Tool then
        Tool.Parent = game:GetService("Players").LocalPlayer.Character
        end
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Play Sound 
        game.Players:chat("/e cm")
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Stop Audio
        function Stop()
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Save Tool
        local Tool = nil
        if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
        Tool = game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
        game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Parent = game:GetService("Players").LocalPlayer.Backpack
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Phone
        game:GetService("Players").LocalPlayer.Backpack["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Character
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Unequip Phone
        game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Backpack
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Saved Tool
        if Tool ~= true then
        if Tool then
        Tool.Parent = game:GetService("Players").LocalPlayer.Character
        end
        end
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Change Id
        function Change(ID)
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Save Tool
        local Tool = nil
        if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
        Tool = game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
        game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Parent = game:GetService("Players").LocalPlayer.Backpack
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Phone
        game:GetService("Players").LocalPlayer.Backpack["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Character
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Fire Event
        local args = {
            [1] = "RingTone",
            [2] = ID
            
        }
        game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Unequip Phone
        game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Backpack
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Saved Tool
        if Tool ~= true then
        if Tool then
        Tool.Parent = game:GetService("Players").LocalPlayer.Character
        end
        end
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Call
        function Call()
        game.Players:chat("/e cm")
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- End
        -- Ringtone(ID)
        -- Stop()
        -- Change(ID)
        -- Call()
    
    
    function kick()
        local KIckAnim = Instance.new('Animation');
        KIckAnim.AnimationId = "rbxassetid://2788838708";
            tool = Instance.new("Tool")
            tool.RequiresHandle = false
            tool.Name = "COD ZOMBIE 👿"
            tool.Activated:Connect(function()
            local gyat = game:GetService('Players').LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(KIckAnim)
            gyat:Play()
            gyat:AdjustSpeed(0.5)
            wait(0.5)
            Ringtone(4846014500)
            for i = 1,20 do
                MoveTo(0.10, 0,0)
                wait(0.1)
                MoveTo(-0.10, 0,0)
                wait(0.2)
            end
            wait(0.1)
            Call()
            Ringtone(1081581251)
            local Humanoid = game:GetService("Players").LocalPlayer.Character.Humanoid;
            Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
            Humanoid.WalkSpeed = _G.WS;
            end)
            Humanoid.WalkSpeed = _G.WS;
                 wait(1)
              _G.WS = "200";
            local Humanoid = game:GetService("Players").LocalPlayer.Character.Humanoid;
            Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
            Humanoid.WalkSpeed = _G.WS;
            end)
            Humanoid.WalkSpeed = _G.WS;
            wait(3)
            local Humanoid = game:GetService("Players").LocalPlayer.Character.Humanoid;
            Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
            Humanoid.WalkSpeed = _G.WS;
            end)
            Humanoid.WalkSpeed = _G.WS;
                 wait(1)
              _G.WS = "20";
            local Humanoid = game:GetService("Players").LocalPlayer.Character.Humanoid;
            Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
            Humanoid.WalkSpeed = _G.WS;
            end)
            Humanoid.WalkSpeed = _G.WS;
            end)
            tool.Parent = game.Players.LocalPlayer.Backpack
        end;
        game:GetService('Players').LocalPlayer.Character:WaitForChild('FULLY_LOADED_CHAR');
        kick()
    end)
