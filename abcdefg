--//------------------------------------------------------------------------------------------\\-- tool
tool.AddToggle("auto reload", false, function(p)
    if p == true then
        game:GetService('RunService'):BindToRenderStep("Auto-Reload", 0 , function()
            if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
                if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool"):FindFirstChild("Ammo") then
                    if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool"):FindFirstChild("Ammo").Value <= 0 then
                        game:GetService("ReplicatedStorage").MainEvent:FireServer("Reload", game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")) 
                        wait(1)
                    end
                end
            end
        end)
    elseif p == false then
        game:GetService('RunService'):UnbindFromRenderStep("Auto-Reload")
    end
end)
tool.AddToggle("Hit Sense", false, function(t)
    if t == true then
    function Ringtone(ID)
    --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Save Tool
    local Tool = nil
    if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
    Tool = game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
    game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Parent = game:GetService("Players").LocalPlayer.Backpack
    end
    --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Phone
    game:GetService("Players").LocalPlayer.Backpack["[Phone]"].Parent =
    game:GetService("Players").LocalPlayer.Character
    --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Fire Event
    local args = {
        [1] = "RingTone",
        [2] = ID
        
    }
    game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
    --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Unequip Phone
    game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent =
    game:GetService("Players").LocalPlayer.Backpack
    --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Saved Tool
    if Tool ~= true then
    if Tool then
    Tool.Parent = game:GetService("Players").LocalPlayer.Character
    end
    end
    --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Play Sound 
    game.Players:chat("/e cm")
    end
    --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Stop Audio
    function Stop()
    --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Save Tool
    local Tool = nil
    if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
    Tool = game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
    game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Parent = game:GetService("Players").LocalPlayer.Backpack
    end
    --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Phone
    game:GetService("Players").LocalPlayer.Backpack["[Phone]"].Parent =
    game:GetService("Players").LocalPlayer.Character
    --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Unequip Phone
    game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent =
    game:GetService("Players").LocalPlayer.Backpack
    --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Saved Tool
    if Tool ~= true then
    if Tool then
    Tool.Parent = game:GetService("Players").LocalPlayer.Character
    end
    end
    end
    --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Change Id
    function Change(ID)
    --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Save Tool
    local Tool = nil
    if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
    Tool = game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
    game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Parent = game:GetService("Players").LocalPlayer.Backpack
    end
    --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Phone
    game:GetService("Players").LocalPlayer.Backpack["[Phone]"].Parent =
    game:GetService("Players").LocalPlayer.Character
    --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Fire Event
    local args = {
        [1] = "RingTone",
        [2] = ID
        
    }
    game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
    --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Unequip Phone
    game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent =
    game:GetService("Players").LocalPlayer.Backpack
    --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Saved Tool
    if Tool ~= true then
    if Tool then
    Tool.Parent = game:GetService("Players").LocalPlayer.Character
    end
    end
    end
    --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Call
    function Call()
    game.Players:chat("/e cm")
    end
    --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- ReCall
    function ReCall()
    Stop()
    game.Players:chat("/e cm")
    end
    --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- End
    -- Ringtone(ID)
    -- Stop()
    -- Change(ID)
    -- Call()
    -- ReCall()
    
        --//Animations\\--
        local Dodge1 = Instance.new('Animation', game.ReplicatedStorage)
        Dodge1.AnimationId = 'rbxassetid://0'
        local Dodge2 = Instance.new('Animation', game.ReplicatedStorage)
        Dodge2.AnimationId = 'rbxassetid://0'
        local Dodge3 = Instance.new('Animation', game.ReplicatedStorage)
        Dodge3.AnimationId = 'rbxassetid://0'
        --//Load Animations\\--
        local d1 = game.Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid'):LoadAnimation(Dodge1)
        local d2 = game.Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid'):LoadAnimation(Dodge2)
        local d3 = game.Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid'):LoadAnimation(Dodge3)
    
    
        local dodge = {
        "Combat",
        "[Knife]",
        "[Nunchucks]",
        "[Bat]",
        "[Katana]",
        "[StopSign]",
        "[SledgeHammer]",
        "[Flamethrower]",
        "[Pitchfork]",
        "[Shovel]"
        }
        
        local function HasTool(tool)  
        for i , v in pairs(dodge) do
        if v == tool then 
        return true 
        end
        end
        end
        local function IsInRage(player, distance)
        if game:GetService("Players").LocalPlayer.Character and game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        local positiondifference = (game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Position - player.Character:FindFirstChild("HumanoidRootPart").Position).Magnitude
        return positiondifference <= distance
        end
        return false
        end
        game:GetService("RunService"):BindToRenderStep("Spidersense", 0 , function()
        for i , player in pairs(game.Players:GetChildren()) do
        if player ~= game.Players.LocalPlayer and IsInRage(player,5) then 
        if player.Character and player.Character:FindFirstChildWhichIsA("Tool") and HasTool(player.Character:FindFirstChildWhichIsA("Tool").Name) and player.Character:FindFirstChild("BodyEffects"):FindFirstChild("Attacking").Value == true then
        local Math = math.random(1,3)
        if Math == 1 and not furrimode then
        furrimode = true
        if game.Players.LocalPlayer.Character.LowerTorso:FindFirstChild("PhoneRing") then
        if game.Players.LocalPlayer.Character.LowerTorso.PhoneRing.SoundId == "rbxassetid://3280066384" then
            game.ReplicatedStorage.MainEvent:FireServer("Block",true)
            wait(.1)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 10)
        ReCall()
        wait(.1)
        game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer("Block",false)
        else
            game.ReplicatedStorage.MainEvent:FireServer("Block",true)
            wait(.1)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 10)
        Ringtone(3280066384)
        wait(.1)
        game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer("Block",false)
        end
        end
        ReCall()
        d1:Play()
        d2:Stop()
        d3:Stop()
        wait(0.50)
        furrimode = false
        elseif Math == 2 and not furrimode then
        furrimode = true 
        if game.Players.LocalPlayer.Character.LowerTorso.PhoneRing.SoundId == "rbxassetid://3280066384" then
            game.ReplicatedStorage.MainEvent:FireServer("Block",true)
            wait(.1)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 10)
        ReCall()
        wait(.1)
        game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer("Block",false)
        else
            game.ReplicatedStorage.MainEvent:FireServer("Block",true)
            wait(.1)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 10)
        Ringtone(3280066384)
        wait(.1)
        game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer("Block",false)
        end
        ReCall()
        d1:Stop()
        d2:Play()
        d3:Stop()
        wait(0.50)
        furrimode = false
        else if Math == 3 and not furrimode then
        furrimode = true
        if game.Players.LocalPlayer.Character.LowerTorso.PhoneRing.SoundId == "rbxassetid://3280066384" then
            game.ReplicatedStorage.MainEvent:FireServer("Block",true)
            wait(.1)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 10)
        ReCall()
        wait(.1)
        game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer("Block",false)
        else
            game.ReplicatedStorage.MainEvent:FireServer("Block",true)
            wait(.1)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 10)
        Ringtone(3280066384)
        wait(.1)
        game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer("Block",false)
        end
        d1:Stop()
        d2:Stop()
        d3:Play()
        wait(0.50)
        furrimode = false
        end
        end
        end
        end
        end
        end)
    elseif t == false then
    game:GetService('RunService'):UnbindFromRenderStep("Spidersense")
    end
end)
tool.AddButton("god spin prass Q",function()
    getgenv().toggle = false
game:GetService("UserInputService").InputBegan:Connect(function(ea , ONG)
if ea.KeyCode == Enum.KeyCode.Q and not ONG then 
getgenv().toggle = not getgenv().toggle
if getgenv().toggle == true then 
    getgenv().loop = game:GetService("RunService").Heartbeat:Connect(function()
    local currentvel = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity
    game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(math.random(0,2000), math.random(0,2000), math.random(0,2000))
    game:GetService("RunService").RenderStepped:Wait()
    game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = currentvel
    end)
    getgenv().loop2 = game:GetService("RunService").RenderStepped:Connect(function()
    game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid").WalkSpeed = 50
    end)
end

    if getgenv().toggle == false then 
    if getgenv().loop then 
       getgenv().loop:Disconnect()
       getgenv().loop2:Disconnect()
                end
            end
    end
end)
end)
tool.AddButton("god blocking",function()
    game:GetService("RunService").stepped:connect(function()
        if game.Players.LocalPlayer.Character.BodyEffects:FindFirstChild('Block') then
            game.Players.LocalPlayer.Character.BodyEffects.Block:Destroy()
        end
        game.ReplicatedStorage.MainEvent:FireServer("Block",true) wait()
        game.ReplicatedStorage.MainEvent:FireServer("Block",false)
    end) 
end)
tool.AddButton("auto Frie armor",function()
    function armorloop()
        while wait() do
            if game.Players.LocalPlayer.Character:WaitForChild('BodyEffects')['FireArmor'].Value <= 0 then
                    local savepos = game.Players.LocalPlayer.Character.UpperTorso.Position
                             local Brokie = game.Workspace.Ignored.Shop["[Fire Armor] - $2493"]
                             game.Players.LocalPlayer.Character:MoveTo(Brokie.Head.Position)
                             wait(0.1)
                             fireclickdetector(Brokie.ClickDetector)
                             wait(0.1)
                      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(savepos)
            end
            end
            end
            
            game.Players.LocalPlayer.Character:WaitForChild("FULLY_LOADED_CHAR")
            armorloop()
            
            game.Players.LocalPlayer.CharacterAdded:connect(function()
                game.Players.LocalPlayer.Character:WaitForChild("FULLY_LOADED_CHAR")
                wait(0.5)
                armorloop()
            end)
        end)
tool.AddButton("auto armor",function()
    function armorloop()
        while wait() do
            if game.Players.LocalPlayer.Character:WaitForChild('BodyEffects')['Armor'].Value <= 30 then
                    local savepos = game.Players.LocalPlayer.Character.UpperTorso.Position
                             local Brokie = game.Workspace.Ignored.Shop["[High-Medium Armor] - $2440"]
                             game.Players.LocalPlayer.Character:MoveTo(Brokie.Head.Position)
                             wait(0.1)
                             fireclickdetector(Brokie.ClickDetector)
                             wait(0.1)
                      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(savepos)
            end
            end
            end
            
            game.Players.LocalPlayer.Character:WaitForChild("FULLY_LOADED_CHAR")
            armorloop()
            
            game.Players.LocalPlayer.CharacterAdded:connect(function()
                game.Players.LocalPlayer.Character:WaitForChild("FULLY_LOADED_CHAR")
                wait(0.5)
                armorloop()
            end)
        end)
