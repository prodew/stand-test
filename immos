false oqf.Anchored = true for dh, v  in pairs(oqf:GetChildren()) do  if v.ClassName ~= "\83\112\101\99\105\97\108\77\101\115\104" then v:Destroy() end end end end local koqf = game:GetObjects("\114\98\120\97\115\115\101\116\105\100\58\47\47\54\53\52\55\48\53\53\53\48\48")[1] koqf.Parent = qfnb koqf.Name = "\73\100\97\114\111" workspace.Camera.CameraSubject = koqf.Humanoid oq = ls.HumanoidRootPart.CFrame hi = true koqf.HumanoidRootPart.CFrame = oq ik = koqf:WaitForChild("\72\117\109\97\110\111\105\100"):LoadAnimation(sgj) ik.Looped = true ik.Priority = Enum.AnimationPriority.Action koq = koqf:WaitForChild("\72\117\109\97\110\111\105\100"):LoadAnimation(gjm) koq.Looped = true koq.Priority = Enum.AnimationPriority.Action _tc = true ik:Play() else hi = false _tc = false workspace.Camera.CameraSubject =  ls.Humanoid ls.HumanoidRootPart.CFrame =  workspace:FindFirstChild(rl.Name.."\83\116\114\105\110\103\75\79\68").Idaro.HumanoidRootPart.CFrame workspace:FindFirstChild(rl.Name.."\83\116\114\105\110\103\75\79\68"):Destroy() end end function k() local nb_tc = ls.Humanoid:LoadAnimation(tc) nb_tc:Play() nb_tc.TimePosition = 0.15 nb_tc.Looped = false nb_tc:AdjustSpeed(1.1) end function i(h, d) d.HumanoidRootPart.Velocity = d.HumanoidRootPart.Velocity+h end local hik = false function e() if hik == false then return false else return true end end local dhik = rl:GetMouse() local ed = Vector3.new(0,0,0) local medh = 0  local jmed = 0 local gjme = 0 function m() if ik and koq.IsPlaying and medh == 0 and jmed == 0 and gjme == 0 then ik:Play() koq:Stop() end end function j() if koq and ik.IsPlaying then ik:Stop() koq:Play() end end dhik.KeyDown:Connect(function(ko) if workspace:FindFirstChild(rl.Name.."\83\116\114\105\110\103\75\79\68") then if ko == "\119" then medh = -1 j() elseif ko == "\97" then jmed = -1 j() elseif ko == "\100" then jmed = 1 j() elseif ko == "\115" then medh = 1 j() end end end) dhik.KeyUp:Connect(function(ko) if ko == "\119" then medh = 0 m() elseif ko == "\97" then jmed = 0 m() elseif ko == "\100" then jmed = -0 m() elseif ko == "\115" then medh = 0 m() end end) local sgjm = false local lsg = game:GetService("\85\115\101\114\73\110\112\117\116\83\101\114\118\105\99\101") lsg.InputBegan:Connect(function(dh, nb_tc) if dh.KeyCode == Enum.KeyCode.H and nb_tc == false then if sg == false then sg = true elseif sg == true then sg = false end end end) lsg.InputBegan:Connect(function(dh, nb_tc) if dh.KeyCode == Enum.KeyCode.Space and nb_tc == false then if workspace:FindFirstChild(rl.Name.."\83\116\114\105\110\103\75\79\68") == nil then if e() == true and not jme.IsPlaying then i(ls.HumanoidRootPart.CFrame.LookVector*150, ls) hik = false pr:Stop() sgjm = true p(6543434995, true, true) k() repeat wait() until workspace:FindPartOnRayWithWhitelist(Ray.new(ls.HumanoidRootPart.Position, Vector3.new(0, -4 * ls.HumanoidRootPart.Size.y, 0)), { workspace.MAP }) sgjm = false end else workspace:FindFirstChild(rl.Name.."\83\116\114\105\110\103\75\79\68"):FindFirstChild("\73\100\97\114\111").Humanoid:ChangeState(3) end end end) function g() ls.LowerTorso.BOOMBOXSOUND:Stop() end function s(c, r) local cprl = coroutine.wrap(function() wait(ls.LowerTorso.BOOMBOXSOUND.TimeLength-0) if ls.LowerTorso.BOOMBOXSOUND.SoundId == "\114\98\120\97\115\115\101\116\105\100\58\47\47"..c and cp == r then g() end end) cprl() end function p(c, t, _) if rl.Backpack:FindFirstChild("\91\66\111\111\109\98\111\120\93") then local prls = nil if ls:FindFirstChildOfClass("\84\111\111\108") and _ == true then prls = ls:FindFirstChildOfClass("\84\111\111\108") ls:FindFirstChildOfClass("\84\111\111\108").Parent = rl.Backpack end rl.Backpack["\91\66\111\111\109\98\111\120\93"].Parent = ls game.ReplicatedStorage.MainEvent:FireServer("\66\111\111\109\98\111\120", c) ls["\91\66\111\111\109\98\111\120\93"].RequiresHandle = false if ls["\91\66\111\111\109\98\111\120\93"]:FindFirstChild("\72\97\110\100\108\101") then ls["\91\66\111\111\109\98\111\120\93"].Handle:Destroy() end ls["\91\66\111\111\109\98\111\120\93"].Parent = rl.Backpack rl.PlayerGui.MainScreenGui.BoomboxFrame.Visible = false if prls ~= true then if prls then prls.Parent = ls end end if t == true then ls.LowerTorso:WaitForChild("\66\79\79\77\66\79\88\83\79\85\78\68") local cprl = coroutine.wrap(function() repeat wait() until ls.LowerTorso.BOOMBOXSOUND.SoundId == "\114\98\120\97\115\115\101\116\105\100\58\47\47"..c and ls.LowerTorso.BOOMBOXSOUND.TimeLength > 0 cp = cp+1 s(c, cp) end) cprl() end end end local tcpr = false local _tcp, CanCel = "\84\105\109\101\32\115\116\111\112\33", "\84\105\109\101\32\114\101\115\117\109\101\33" local b_tc, TimeResume = "\84\105\109\101\32\115\116\111\112\33", "\84\105\109\101\32\115\116\111\112\33" local nb_tc = false rl.Chatted:Connect(function(me) if me == "\82\97\103\101\33" then if qfn:FindFirstChild("\72\101\97\100\83\99\97\108\101") then b() wait(0.6) qfn:FindFirstChild("\72\101\97\100\83\99\97\108\101"):Destroy() wait(0.6) end if qfn:FindFirstChild("\66\111\100\121\87\105\100\116\104\83\99\97\108\101") then b() wait(0.6) qfn:FindFirstChild("\66\111\100\121\87\105\100\116\104\83\99\97\108\101"):Destroy() wait(0.6) end if qfn:FindFirstChild("\66\111\100\121\68\101\112\116\104\83\99\97\108\101") then b() wait(0.6) qfn:FindFirstChild("\66\111\100\121\68\101\112\116\104\83\99\97\108\101"):Destroy() wait(0.6) end if qfn:FindFirstChild("\66\111\100\121\84\121\112\101\83\99\97\108\101") then b() wait(0.6) qfn:FindFirstChild("\66\111\100\121\84\121\112\101\83\99\97\108\101"):Destroy() wait(0.6) end elseif me:sub(1, 11) == _tcp and nb_tc == false and not jme.IsPlaying then b_(nb_tc) elseif me == CanCel and nb_tc == true and not jme.IsPlaying then b_(nb_tc) elseif me == b_tc then p(170278900, true, true) wait(0.3) ls.LowerTorso.BOOMBOXSOUND.SoundId = "\104\116\116\112\58\47\47\119\119\119\46\114\111\98\108\111\120\46\99\111\109\47\97\115\115\101\116\47\63\105\100\61\119\100" wait(0.1) p(7084712646, true, true) end end) function b_(nb) if nb == false then nb_tc = true p(4786770367, true, true) wait(2) q() n() f() o() else nb_tc = false hi = false wait(0.1) p(4463896146, true, true) q() o() f() n() end end function Jump(...) (...).Jumping:Connect(function() if sg == true then if sgjm == false and not jme.IsPlaying then sgjm = true delay(1, function() sgjm = false end) pr:Play(0, 1, 1.4) i(ls.HumanoidRootPart.CFrame.UpVector*120, ls) p(8296182640, true, true) delay(0, function() hik = true wait(1)  if hik == true then hik = false end end) end end end) end Jump(ls.Humanoid) rl.CharacterAdded:Connect(function(jm) wait(1) if jm.Name == rl.Name then Jump(jm.Humanoid) jme = jm:WaitForChild("\72\117\109\97\110\111\105\100"):LoadAnimation(dhi) jme.Looped = true jme.Priority = Enum.AnimationPriority.Action pr = jm:WaitForChild("\72\117\109\97\110\111\105\100"):LoadAnimation(game.ReplicatedStorage.ClientAnimations.Roll) ls = jm end end) game["\82\117\110\32\83\101\114\118\105\99\101"].Heartbeat:Connect(function() if hi then for dh, v in pairs(ls:GetChildren()) do if v:IsA("\66\97\115\101\80\97\114\116") then v.CFrame = CFrame.new(math.random(200, 500), math.random(200, 500), math.random(200, 500)) v.Velocity = Vector3.new(0,0,0) end end if ls:FindFirstChild("\67\111\109\98\97\116") and nb_tc == true then nb_tc = false b_(true) end if _tc == true then ed = Vector3.new(jmed, gjme, medh) workspace:FindFirstChild(rl.Name.."\83\116\114\105\110\103\75\79\68"):FindFirstChild("\73\100\97\114\111").Humanoid:Move(ed, true) end else if ls:FindFirstChild("\91\70\108\97\115\104\108\105\103\104\116\93") then if workspace.Players:FindFirstChild(gj) then if not jme.IsPlaying then jme:Play() jme:AdjustSpeed(0) jme.TimePosition = 1.2 end local fnb_t = workspace.Players:FindFirstChild(gj).HumanoidRootPart.CFrame * CFrame.new(0, 0.5, 1.25) ls.HumanoidRootPart.CFrame = fnb_t end else if jme.IsPlaying then jme:Stop() end end end end)
end)
local MainButton = MainPage.AddButton("Grab", function()
    local number = 3 --faster movement. less bounceback.


function sandbox(var,func)
	local env = getfenv(func)
	local newenv = setmetatable({},{
		__index = function(self,k)
			if k=="script" then
				return var
			else
				return env[k]
			end
		end,
	})
	setfenv(func,newenv)
	return func
end
cors = {}
mas = Instance.new("Model",game:GetService("Lighting"))
Tool0 = Instance.new("Tool")
Part1 = Instance.new("Part")
Script2 = Instance.new("Script")
LocalScript3 = Instance.new("LocalScript")
Tool0.Name = "Grab"
Tool0.Parent = mas
Tool0.Grip = CFrame.new(0, 0, 1, 1, 0, 0, 0, 1, 0, 0, 0, 1)
Tool0.GripPos = Vector3.new(0, 0, 1)
Part1.Name = "Handle"
Part1.Parent = Tool0
Part1.CFrame = CFrame.new(-3.5, 5.30000019, -3.5, 1, 0, 0, 0, -1, 0, 0, 0, -1)
Part1.Orientation = Vector3.new(0, 180, 180)
Part1.Position = Vector3.new(-3.5, 5.300000190734863, -3.5)
Part1.Rotation = Vector3.new(-180, 0, 0)
Part1.Color = Color3.new(0.972549, 0.972549, 0.972549)
Part1.Transparency = 1
Part1.Size = Vector3.new(1, 1, 1)
Part1.BottomSurface = Enum.SurfaceType.Smooth
Part1.BrickColor = BrickColor.new("Institutional white")
Part1.Locked = true
Part1.TopSurface = Enum.SurfaceType.Smooth
Part1.brickColor = BrickColor.new("Institutional white")
Script2.Name = "LineConnect"
Script2.Parent = Tool0
table.insert(cors,sandbox(Script2,function()
	wait()
	local check = script.Part2
	local part1 = script.Part1.Value
	local part2 = script.Part2.Value
	local parent = script.Par.Value
	local color = script.Color
	local line = Instance.new("Part")
	line.TopSurface = 0
	line.BottomSurface = 0
	line.Reflectance = .5
	line.Name = "Laser"
	line.Locked = true
	line.CanCollide = false
	line.Anchored = true
	line.formFactor = 0
	line.Size = Vector3.new(1,1,1)
	local mesh = Instance.new("BlockMesh")
	mesh.Parent = line
	while true do
		if (check.Value==nil) then break end
		if (part1==nil or part2==nil or parent==nil) then break end
		if (part1.Parent==nil or part2.Parent==nil) then break end
		if (parent.Parent==nil) then break end
		local lv = CFrame.new(part1.Position,part2.Position)
		local dist = (part1.Position-part2.Position).magnitude
		line.Parent = parent
		line.BrickColor = color.Value.BrickColor
		line.Reflectance = color.Value.Reflectance
		line.Transparency = color.Value.Transparency
		line.CFrame = CFrame.new(part1.Position+lv.lookVector*dist/2)
		line.CFrame = CFrame.new(line.Position,part2.Position)
		mesh.Scale = Vector3.new(.25,.25,dist)
		wait()
	end
	line:remove()
	script:remove() 
end))
Script2.Disabled = true
LocalScript3.Name = "MainScript"
LocalScript3.Parent = Tool0
table.insert(cors,sandbox(LocalScript3,function()
	wait() 
	tool = script.Parent
	lineconnect = tool.LineConnect
	object = nil
	mousedown = false
	found = false
	BP = Instance.new("BodyPosition")
	BP.maxForce = Vector3.new(math.huge*math.huge,math.huge*math.huge,math.huge*math.huge) --pwns everyone elses bodyposition
	BP.P = BP.P*number
	dist = nil
	point = Instance.new("Part")
	point.Locked = true
	point.Anchored = true
	point.formFactor = 0
	point.Shape = 0
	point.BrickColor = BrickColor.Black() 
	point.Size = Vector3.new(1,1,1)
	point.CanCollide = false
	local mesh = Instance.new("SpecialMesh")
	mesh.MeshType = "Sphere"
	mesh.Scale = Vector3.new(.7,.7,.7)
	mesh.Parent = point
	handle = tool.Handle
	front = tool.Handle
	color = tool.Handle
	objval = nil
	local hooked = false 
	local hookBP = BP:clone() 
	hookBP.maxForce = Vector3.new(30000,30000,30000) 

	function LineConnect(part1,part2,parent)
		local p1 = Instance.new("ObjectValue")
		p1.Value = part1
		p1.Name = "Part1"
		local p2 = Instance.new("ObjectValue")
		p2.Value = part2
		p2.Name = "Part2"
		local par = Instance.new("ObjectValue")
		par.Value = parent
		par.Name = "Par"
		local col = Instance.new("ObjectValue")
		col.Value = color
		col.Name = "Color"
		local s = lineconnect:clone()
		s.Disabled = false
		p1.Parent = s
		p2.Parent = s
		par.Parent = s
		col.Parent = s
		s.Parent = workspace
		if (part2==object) then
			objval = p2
		end
	end

	function onButton1Down(mouse)
		if (mousedown==true) then return end
		mousedown = true
		coroutine.resume(coroutine.create(function()
			local p = point:clone()
			p.Parent = tool
			LineConnect(front,p,workspace)
			while (mousedown==true) do
				p.Parent = tool
				if (object==nil) then
					if (mouse.Target==nil) then
						local lv = CFrame.new(front.Position,mouse.Hit.p)
						p.CFrame = CFrame.new(front.Position+(lv.lookVector*1000))
					else
						p.CFrame = CFrame.new(mouse.Hit.p)
					end
				else
					LineConnect(front,object,workspace)
					break
				end
				wait()
			end
			p:remove()
		end))
		while (mousedown==true) do
			if (mouse.Target~=nil) then
				local t = mouse.Target
				if (t.Anchored==false) then
					object = t
					dist = (object.Position-front.Position).magnitude
					break
				end
			end
			wait()
		end
		while (mousedown==true) do
			if (object.Parent==nil) then break end
			local lv = CFrame.new(front.Position,mouse.Hit.p)
			BP.Parent = object
			BP.position = front.Position+lv.lookVector*dist
			wait()
		end
		BP:remove()
		object = nil
		objval.Value = nil
	end

	function onKeyDown(key,mouse) 
		local key = key:lower() 
		local yesh = false 
		if (key=="q") then 
			if (dist>=5) then 
				dist = dist-5 
			end 
		end
		if (key=="u") then 
			if (dist ~=1) then 
				BX = Instance.new("BodyGyro")
				BX.MaxTorque = Vector3.new(math.huge,0,math.huge)
				BX.CFrame = BX.CFrame * CFrame.Angles(0, math.rad(45), 0)
				BX.D = 0
				BX.Parent = object
				--object.CFrame = CFrame.Angles(math.rad(90), 0,0) -- not used
				--object.Orientation = Vector3.new(50,0,0)
				--BX.Orientation = Vector3.new(23,0,0)

			end 
		end 
		if (key=="p") then 
			if (dist ~=1) then
				BX = Instance.new("BodyVelocity")
				BX.maxForce = Vector3.new(0,math.huge,0)
				BX.velocity = Vector3.new(0,1,0)
				--BX.CFrame = BX.CFrame * CFrame.Angles(0, math.rad(45), 0)
				BX.Parent = object

			end 
		end 
		if key == "l" then 
			if (object==nil) then return end 
			for _,v in pairs(object:children()) do 
				if v.className == "BodyGyro" then 
					return nil 
				end 
			end 
			BG = Instance.new("BodyGyro") 
			BG.maxTorque = Vector3.new(math.huge,math.huge,math.huge) 
			BG.cframe = CFrame.new(object.CFrame.p) 
			BG.Parent = object 
			repeat wait() until(object.CFrame == CFrame.new(object.CFrame.p))
			BG.Parent = nil 
			if (object==nil) then return end 
			for _,v in pairs(object:children()) do 
				if v.className == "BodyGyro" then 
					v.Parent = nil 
				end 
			end 
			object.Velocity = Vector3.new(0,0,0) 
			object.RotVelocity = Vector3.new(0,0,0) 
		end 
		if (key=="y") then 
			if (dist ~=100) then 
				dist = 100 
			end 
		end 
		if (key=="j") then 
			if (dist~=500) then 
				dist = 500 
			end 
		end
		if (key=="e") then
			dist = dist+5
		end
		if (key=="x") then 
			if dist ~= 15 then 
				dist = 15 
			end 
		end 
	end

	function onEquipped(mouse)
		keymouse = mouse
		local char = tool.Parent
		human = char.Humanoid
		human.Changed:connect(function() if (human.Health==0) then mousedown = false BP:remove() point:remove() tool:remove() end end)
		mouse.Button1Down:connect(function() onButton1Down(mouse) end)
		mouse.Button1Up:connect(function() mousedown = false end)
		mouse.KeyDown:connect(function(key) onKeyDown(key,mouse) end)
		mouse.Icon = "rbxasset://textures\\GunCursor.png"
	end

	tool.Equipped:connect(onEquipped)
end))
for i,v in pairs(mas:GetChildren()) do
	v.Parent = game:GetService("Players").LocalPlayer.Backpack
	pcall(function() v:MakeJoints() end)
end
mas:Destroy()
for i,v in pairs(cors) do
	spawn(function()
		pcall(v)
	end)
end
end)
