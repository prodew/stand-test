MainPage.AddButton("Da Better Stomp", function()
    keybind = "e"
    player = game.Players.LocalPlayer
    mouse = player:GetMouse()
    mouse.KeyDown:connect(function(key)
    if key == keybind then
    wait(1.1)
    local Math = math.random(1,9)
    if Math == 1 then
    pcall(function()
    for _, v in pairs(game:GetService('Players'):GetPlayers()) do
    if v.Name ~= game:GetService('Players').LocalPlayer.Name then
    if v.Character:FindFirstChild('BodyEffects'):FindFirstChild('K.O') then
    for _, v in pairs(v.Character:GetChildren()) do
    if v:IsA('BasePart') then
    v.Velocity = Vector3.new(10,150,10);
    end
    end;
    end;
    end;
    end;
    end);
    elseif Math == 2 then
        pcall(function()
            for _, v in pairs(game:GetService('Players'):GetPlayers()) do
            if v.Name ~= game:GetService('Players').LocalPlayer.Name then
            if v.Character:FindFirstChild('BodyEffects'):FindFirstChild('K.O') then
            for _, v in pairs(v.Character:GetChildren()) do
            if v:IsA('BasePart') then
            v.Velocity = Vector3.new(70,150,10);
            end
            end;
            end;
            end;
            end;
            end);
    elseif Math == 3 then
        pcall(function()
