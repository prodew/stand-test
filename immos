	tool.Activated:connect(function()
		local A_1 = game.Players.LocalPlayer.Character.BodyEffects.Movement:FindFirstChild("SuperSpeed")
		local A_2 = game.Players.LocalPlayer.Character.BodyEffects.Movement:FindFirstChild("HulkJump")
		if A_1 then
pcall(function()
for _, v in pairs(game:GetService('Players'):GetPlayers()) do
if v.Name ~= game:GetService('Players').LocalPlayer.Name then
if v.Character:FindFirstChild('BodyEffects'):FindFirstChild('K.O') then
