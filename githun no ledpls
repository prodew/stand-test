MIs.AddButton("remove stand zer1ckq!",function()
    local Alt = "zer1ckq"

    game.Players[Alt].Character.RightUpperArm.Position = Vector3.new(0,-1200,0)
    
    if game.Players.LocalPlayer.Name == "zer1ckq" then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.RKLWPGZLG.Character.HumanoidRootPart.CFrame * CFrame.new(0,0,-2)
        local person = "RKLWPGZLG"
        game.Players[person].Character.BodyEffects["K.O"]:GetPropertyChangedSignal("Value"):Wait()
        if game.Players[person].Character:FindFirstChild("GRABBING_CONSTRAINT") then
        else
        wait(0.3)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game.Players[person].Character.UpperTorso.Position.X,game.Players[person].Character.UpperTorso.Position.Y,game.Players[person].Character.UpperTorso.Position.Z) * CFrame.new(0,1,0)
        task.wait(0.2)
        local args = {
            [1] = "Grabbing",
            [2] = true
        }
     
        game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer(unpack(args))
        end
        wait(0.7)
        if game.Players[person].Character:FindFirstChild("GRABBING_CONSTRAINT") then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0, -499, 0)
        end
        end
    
end)
