tool.AddButton("thunder",function()
    function Ringtone(ID)
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Save Tool
        local Tool = nil
        if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
        Tool = game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
        game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Parent = game:GetService("Players").LocalPlayer.Backpack
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Phone
        game:GetService("Players").LocalPlayer.Backpack["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Character
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Fire Event
        local args = {
            [1] = "RingTone",
            [2] = ID
            
        }
        game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Unequip Phone
        game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Backpack
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Saved Tool
        if Tool ~= true then
        if Tool then
        Tool.Parent = game:GetService("Players").LocalPlayer.Character
        end
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Play Sound 
        game.Players:chat("/e cm")
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Stop Audio
        function Stop()
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Save Tool
        local Tool = nil
        if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
        Tool = game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
        game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Parent = game:GetService("Players").LocalPlayer.Backpack
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Phone
        game:GetService("Players").LocalPlayer.Backpack["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Character
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Unequip Phone
        game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Backpack
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Saved Tool
        if Tool ~= true then
        if Tool then
        Tool.Parent = game:GetService("Players").LocalPlayer.Character
        end
        end
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Change Id
        function Change(ID)
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Save Tool
        local Tool = nil
        if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
        Tool = game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
        game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Parent = game:GetService("Players").LocalPlayer.Backpack
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Phone
        game:GetService("Players").LocalPlayer.Backpack["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Character
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Fire Event
        local args = {
            [1] = "RingTone",
            [2] = ID
            
        }
        game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Unequip Phone
        game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent =
        game:GetService("Players").LocalPlayer.Backpack
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Saved Tool
        if Tool ~= true then
        if Tool then
        Tool.Parent = game:GetService("Players").LocalPlayer.Character
        end
        end
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Call
        function Call()
        game.Players:chat("/e cm")
        end
        --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- End
        -- Ringtone(ID)
        -- Stop()
        -- Change(ID)
        -- Call()

        getgenv().sadfasfrfgrthrtyhj = 310
        getgenv().rthrthrthrjrt = 400
     
        game:GetService("RunService").heartbeat:Connect(function()
            for _, v in next, game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):GetPlayingAnimationTracks() do
                if (v.Animation.AnimationId:match("rbxassetid://14496531574")) then
                    local Vel = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity
                    game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector.X*getgenv().sadfasfrfgrthrtyhj,getgenv().sadfasfrfgrthrtyhj,game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector.Z*getgenv().sadfasfrfgrthrtyhj)
                    game:GetService("RunService").RenderStepped:Wait()
                    game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vel
                end
            end
        end)
    
         
        function kick()
            local KIckAnim = Instance.new('Animation');
            KIckAnim.AnimationId = "rbxassetid://14496531574";
                tool = Instance.new("Tool")
