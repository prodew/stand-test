 function battool()
    Game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=game.Workspace.Ignored.Shop["[Bat] - $265"].Head.CFrame*CFrame.new(0,-5,0)
    fireclickdetector(game.Workspace.Ignored.Shop["[Bat] - $265"].ClickDetector)
    Game.Players.LocalPlayer.Character.Backpack:FindFirstChild("[Bat]")
    Game.Players.LocalPlayer.Character.Backpack:FindFirstChild("[Bat]").Parent = Game.Players.LocalPlayer.Character
    Game.Players.LocalPlayer.Character.Character:FindFirstChild('[Bat]'):Activate()
 end
 BT.AddButton("BAT",function()
    battool()
end)
 BT.AddButton("bat two!",function()
local b = 2
----------------------------------------------------------------------------------------------
local a = {}
local savepos = game.Players.LocalPlayer.Character.UpperTorso.Position
repeat
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Ignored.Shop["[Bat] - $265"].Head.CFrame + Vector3.new(0, 3, 0)
    wait(0.6)
    fireclickdetector(game:GetService("Workspace").Ignored.Shop["[Bat] - $265"].ClickDetector)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(savepos)
    for i, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
        if v.Name == "[Bat]" then
            v.Parent = game.Players.LocalPlayer.Character
            table.insert(a, v)
        end
    end
until #a >= b
local c = -6
for i, v in next, game.Players.LocalPlayer.Character:GetChildren() do
    if v.Name == "[Bat]" then
        v.Parent = game.Players.LocalPlayer.Backpack
        v.GripPos = Vector3.new(0, c, 0)
        c = c + 4
        v.GripForward = Vector3.new(0, 0, 0)
        v.GripRight = Vector3.new(0, 0, 0)
        v.GripUp = Vector3.new(0, 100, 0)
        v.Parent = game.Players.LocalPlayer.Character
    end
end
    end)

